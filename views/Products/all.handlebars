<h2>Produtos cadastrados</h2>
<div class="products">
    {{#each products}}
    <div class="product">
        <img src="{{this.image}}" alt="{{this.name}}">
        <h3>{{this.name}}</h3>
        <p>R${{this.price}},00</p>
        <div class="actions">
            <a href="/{{this._id}}">Detalhes</a>
            <a href="/edit/{{this._id}}">Editar</a>
        </div>
    </div>
    {{/each}}
</div>